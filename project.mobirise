            "<script src=\"https://www.jsviews.com/download/jsrender.min.js\"></script>",
            "<script src=\"https://unpkg.com/isotope-layout@3/dist/isotope.pkgd.min.js\"></script>",
            "<script>\n    function winLoad(callback) {\n        if (document.readyState === 'complete') {\n            callback();\n        }\n        else {\n            window.addEventListener(\"load\", callback);\n        }\n    }\n\n    winLoad(\n        function () {\n\n            let datasource = \"https://seedslive.genblue.workers.dev/people?view=Speakers%20%28published%29\";\n\n            getData(datasource, (data) => {\n                console.log({ data })\n\n                var $ = window.jsrender;                                  // JsRender namespace, used instead of jQuery object \n                var markup = document.getElementById(\"speakersTmpl\").innerHTML; // Get template markup from script block contents\n                var tmpl = $.templates(markup);                           // Compile template from markup string\n\n\n                var html = tmpl.render(data);                             // Render template using data - as HTML string\n                document.getElementById(\"result\").innerHTML = html;\n\n                // ISOTOPE\n                var elem = document.querySelector('.grid');\n                var iso = new Isotope(elem, {\n                    // options\n                    layoutMode: 'fitRows',\n                    fitRows: {\n                        gutter: 10\n                    }\n\n                });\n            });\n        }\n    );\n\n    function getData(url, cb) {\n        fetch(url)\n            .then(response => response.json())\n            .then(result => cb(result));\n    }\n</script>",
            "<script id=\"speakersTmpl\" type=\"text/x-jsrender\">\n\n    {{for records}}                        \n        <div class=\"skill-card\">\n            <header class=\"skill-card__header\"><img class=\"skill-card__icon\"\n                    src=\"{{:fields.Attachments[0].thumbnails.large.url}}\" alt=\"HTML5 Logo\" /></header>\n            <section class=\"skill-card__body\">\n                <h2 class=\"skill-card__title\">{{:fields.Name}}</h2><span class=\"skill-card__duration\">{{:fields.Affiliation}}</span>\n                <p class=\"skill-card__knowledge\">\n                    {{:fields.Bio}}\n                </p>\n                <ul class=\"skill-card__knowledge\">\n                    <li>HTML5</li>\n                    <li>Sémantique</li>\n                    <li>SVG</li>\n                    <li>Canvas</li>\n                </ul>\n            </section>\n        </div>\n    {{/for}}\n    \n</script>"
          ]
        }
      ]
